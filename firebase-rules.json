{
  "rules": {
    "sessions": {
      "$sessionId": {
        ".read": "auth != null",
        ".write": "auth != null",
        "participants": {
          "$userId": {
            ".write": "$userId == auth.uid"
          }
        }
      }
    },
    "chats": {
      "$sessionId": {
        ".read": "auth != null",
        ".write": "auth != null"
      }
    }
  }
}